package Values;

public class setValues {
	
	
	public static<K> void setValues(K[] field, int values, int choice) {
		switch (choice) {
		case 1:
			for (int i = values-1; i > -1; --i)
				field[i] = i;
			break;
		case 2:
			for (int i = 0; i < values; ++i)
				field[i] = (K) (Math.random() * 1000000);
			break;
		case 3:
			for (int i = 0; i < values; ++i)
				field[i] = i;
			break;
		}
		
	}
}
